{
  "name": "DrillDragonHelper",
  "productName": "DrillDragonHelper",
  "description": "Portable Dragon Helper Desktop Guy",
  "keywords": [],
  "main": "./main.js",
  "version": "0.9.0",
  "author": "Evan Nave",
  "scripts": {
    "start": "electron .",
    "build": "npx electron-builder build --win portable"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "three": "^0.165.0",
    "three-stdlib": "^2.30.3",
    "three.js": "0.77.1"
  },
  "devDependencies": {
    "electron": "30.0.2",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.mrpalland.DragonHelper",
    "productName": "DrillDragonHelper",
    "win": {
      "icon": "assets/icons/Drilly.ico",
      "target": [
        "portable"
      ],
      "extraResources": [
        {
          "from": "assets/",
          "to": "assets/",
          "filter": [
            "**/*"
          ]
        }
      ]
    }
  }
}
